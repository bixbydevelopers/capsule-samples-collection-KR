capsule {
  id (example.masksales)
  version (1.0.0)
  format (3)
  targets {
    target (bixby-mobile-ko-KR)
  }
  runtime-version (7)
  store-countries {
    all
  }
  device-models {
    all
  }
  capsule-imports {
    import (viv.geo) { as (geo) version (9.26.40) }
    import (viv.entity) { as (entity) version(4.0.14) }
    import (viv.core) { as (core) version-from (entity) }
  }
  store-sections {
    section (Local)
  }
  permissions {
    device-location-access
  }
}
