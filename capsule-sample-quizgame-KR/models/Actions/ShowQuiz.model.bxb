action (ShowQuiz) {
  description (퀴즈 문제를 보여줍니다)
  type(Constructor)
  collect {
    input (quizlist) {
      type (ProgressInfo)
      min (Required) max (One)
      default-init{
        intent{
          goal: GetQuizzes
        }          
      }
    }
    input(answer){
      type(QuizAnswers)
      min (Optional) max (One)
    }
    input(ordinal){
      type(entity.Ordinal)
      min (Optional) max (One)
    }
  }
  output (ProgressInfo)
}
